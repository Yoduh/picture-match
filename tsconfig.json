{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src",
    "typeRoots": ["./node_modules/@types", "./src/@types"],
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "vite.config.ts",
    "src/stores/index.js"
  ]
}
